<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ICR.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.EntranceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.LogoutActivity" />
        <activity android:name=".activity.PasswordRelativeActivity" />
        <activity android:name=".activity.PersonalInformationActivity" />
        <activity android:name=".activity.IdentifyActivity" />
        <activity android:name=".activity.PermissionActivity" />
        <activity android:name=".activity.FinishedActivity" />
        <activity android:name=".activity.StatusActivity" />
        <activity android:name=".activity.ReserveActivity" />
        <activity android:name=".activity.FeedbackActivity"></activity>
    </application>

</manifest>